{
  "name": "ecom-app-now-server",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "description": "apollo-server-ts, jwt, nodemailer, mongo-atlas, express",
  "repository": "git@github.com:quborg/e-comm-server.git",
  "contributors": [
    {
      "name": "Miloud Lafttah",
      "email": "miloud.lafttah@gmail.com"
    }
  ],
  "engines": {
    "yarn": ">=1.22.x"
  },
  "main": "dist/index.production.cjs",
  "type": "module",
  "scripts": {
    "dist": "BUILD_ONLY=1 webpack --config config/webpack/prod.ts",
    "dist-run": "node ./dist/index.production.cjs",
    "start": "yarn dev",
    "prod": "webpack --config config/webpack/prod.ts",
    "dev": "webpack --config config/webpack/dev.ts",
    "lint": "eslint '*/**/*.ts' --quiet --fix",
    "format": "prettier --write '*/**/*.ts'",
    "pre-commit-msg": "echo 'Pre-commit linting errors checks...' && exit 0"
  },
  "pre-commit": {
    "onDirtyState": "fail",
    "run": [
      "pre-commit-msg",
      "format",
      "lint"
    ]
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.4",
    "@types/webpack-env": "^1.16.0",
    "@types/webpack-node-externals": "^2.5.1",
    "apollo-server": "^2.22.2",
    "bcryptjs": "^2.4.3",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.10",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "nodemailer": "^6.6.1",
    "run-node-webpack-plugin": "^1.3.0",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0",
    "@types/nodemailer": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-loader": "^2.1.2",
    "nodemon-webpack-plugin": "^4.5.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "saslprep": "^1.0.3",
    "typescript": "^4.2.4"
  }
}
